{
  "_id": "{{@ system @}}-{{@ datatype @}}-collect",
  "namespaced_identifiers": false,
  "source": {
    "operation": "{{@ datatype @}}-list",
    "system": "{{@ system @}}",
    "type": "rest"
  },
  "transform": {
    "rules": {
      "default": [
        [
          "merge",
          "_S.node"
        ],
        [
          "add",
          "$last-modified",
          [
            "datetime",
            "_S.node.modifiedAt"
          ]
        ]
      ]
    },
    "type": "dtl"
  },
  "type": "pipe"
}